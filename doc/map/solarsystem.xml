  </link>
  <link type="circle">
    <node name="neptune1"/>
    <node name="neptune2"/>
    <node name="pluto"/>
  </link>
  <link type="line">
    <node name="neptune2"/>
    <node name="deathstar2"/>
    <node name="pluto"/>
  </link>
 <!-- intercontinental links -->
  <link type="clique">
    <node name="mars1"/>
    <node name="mars2"/>
    <node name="tellus1"/>
    <node name="tellus2"/>
  </link>
  <link type="clique">
    <node name="jupiter3"/>
    <node name="jupiter5"/>
    <node name="saturn1"/>
    <node name="saturn3"/>
  </link>
  <link type="line">
    <node name="neptune1"/>
    <node name="uranus2"/>
    <node name="neptune2"/>
  </link>
  <link type="line">
    <node name="pluto"/>
    <node name="mercury"/>
  </link>
  <link type="line">
    <node name="deathstar3"/>
    <node name="jupiter5"/>
  </link>
  <link type="line">
    <node name="uranus3"/>
    <node name="deathstar2"/>
    <node name="saturn3"/>
  </link>
  
</map>
