<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE map SYSTEM "file:../file-formats/map-file.dtd">

<map bg="doc/map/solarsystem.png" 
     xscale="4" yscale="2.8" xoffset="0" yoffset="150" zoom="2">
  <meta>
    <author>The JAGSAT Development team.</author>
    <description>solarsystem map.</description>
  </meta>

  <!-- continent mercury, venus, tellus -->

  <continent name="mercury venus and tellus"
	     troops="4">
    <region name="mercury">
      <circle>
	<point x="46" y="75"/>
